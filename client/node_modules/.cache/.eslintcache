iews.js",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/screens/Books.jsx",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/screens/BookDetails.jsx",["95"],"import { useState, useEffect } from \"react\";\nimport { useParams, Link } from \"react-router-dom\";\nimport { getOneBook } from \"../services/books\";\n\nimport ReviewsContainer from \"../containers/ReviewsContainer\";\n\nfunction BookDetails(props) {\n  const [book, setBook] = useState(null);\n  const { id } = useParams();\n  const { currentUser, handleDelete } = props;\n\n  useEffect(() => {\n    const fetchBook = async () => {\n      const bookData = await getOneBook(id);\n      setBook(bookData);\n    };\n    fetchBook();\n  }, []);\n\n  return (\n    <>\n      <div className=\"book-details\">\n        <img src={book?.image_url} alt={book?.title} />\n        <div className=\"book-details2\">\n          <div>\n            <h5>{book?.title}</h5>\n            <h4>by {book?.author}</h4>\n            <br></br>\n          <p>{book?.summary}</p>\n          </div>\n          {currentUser?.id === book?.user_id && (\n            <div className=\"details-buttons\">\n              <Link to={`/books/${id}/edit`}>\n                <button className=\"edit\">EDIT</button>\n              </Link>\n              <button onClick={() => handleDelete(book.id)}>DELETE</button>\n            </div>\n          )}\n        </div>\n      </div>\n      <ReviewsContainer book={book} id={id} currentUser={currentUser} />\n      <br></br>\n      <br></br>\n    </>\n  );\n}\n\nexport default BookDetails;\n","/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/components/Card.jsx",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/screens/BookCreate.jsx",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/screens/BookEdit.jsx",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/components/Reviews.jsx",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/containers/ReviewsContainer.jsx",["96"],"import React, { useState, useEffect } from 'react';\n// import { useHistory } from \"react-router-dom\"; \nimport ReviewCreate from \"../components/ReviewCreate\"\nimport Reviews from \"../components/Reviews\"\nimport { getAllReviews, destroyReview, putReview, postReview } from '../services/reviews';\n\nfunction ReviewsContainer(props) {\n  const { book, id, currentUser } = props;\n  const [reviews, setReviews] = useState([])\n  // const history = useHistory(); \n\n  useEffect(() => {\n    const fetchReviews = async (bookId) => {\n      const reviewData = await getAllReviews(bookId);\n      setReviews(reviewData);\n    };\n    fetchReviews(id);\n  }, []);\n\n  const reviewCreate = async (bookId, reviewData) => {\n    const newReview = await postReview(bookId, reviewData);\n    setReviews((prevState) => [...prevState, newReview]);\n  };\n\n  const reviewUpdate = async (bookId, id, reviewData) => {\n    const updatedReview = await putReview(bookId, id, reviewData)\n    setReviews(prevState => prevState.map(review => {\n      return review.id === Number(id) ? updatedReview : review\n    }))\n  }\n\n  const reviewDelete = async (bookId, id) => {\n    await destroyReview(bookId, id);\n    setReviews(prevState => prevState.filter(review => review.id !== id))\n  }\n\n  return (\n    <div className=\"reviews-container\">\n      <ReviewCreate book={book} reviews={reviews} reviewCreate={reviewCreate} currentUser={currentUser}/>\n      <Reviews book={book} reviews={reviews} currentUser={currentUser} reviewUpdate={reviewUpdate} reviewDelete={reviewDelete} />\n      \n    </div>\n  );\n}\n\nexport default ReviewsContainer;","/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/components/ReviewCreate.jsx",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/components/ReviewEdit.jsx",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/components/Search.jsx",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/components/Nav.jsx",[],"/Users/brittanyowen/General_Assembly/sei/sapphire/unit_4/the-book-nook/client/src/components/Footer.jsx",[],{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","severity":1,"message":"102","line":18,"column":6,"nodeType":"103","endLine":18,"endColumn":8,"suggestions":"104"},{"ruleId":"101","severity":1,"message":"102","line":18,"column":6,"nodeType":"103","endLine":18,"endColumn":8,"suggestions":"105"},"no-native-reassign",["106"],"no-negated-in-lhs",["107"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'id'. Either include it or remove the dependency array.","ArrayExpression",["108"],["109"],"no-global-assign","no-unsafe-negation",{"desc":"110","fix":"111"},{"desc":"110","fix":"112"},"Update the dependencies array to be: [id]",{"range":"113","text":"114"},{"range":"115","text":"114"},[515,517],"[id]",[636,638]]